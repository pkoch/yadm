alias y=yadm
alias ls='ls -GFhb'
alias la='ls -a'
alias ll='la -pl'
alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'
alias defbro-ff='defbro org.mozilla.firefox'
alias defbro-c='defbro com.google.Chrome'
